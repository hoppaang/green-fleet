<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<application>green-fleet</application>
	<version>1</version>

	<!-- By default, App Engine sends requests serially to a given web server. 
		To allow App Engine to send multiple requests in parallel specify: <threadsafe>true</threadsafe> -->
	<threadsafe>true</threadsafe>

	<!-- Configure java.util.logging -->
	<system-properties>
		<property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
	</system-properties>

	<static-files>
		<include path="/*.js"/>
		<include path="/*.html"/>
		<include path="/app/**.*"/>
		<include path="/data/**.*"/>
		<include path="/resources/css/ext-all.css" />
		<include path="/resources/themes/images/default/**.*" />
	</static-files>

 </appengine-web-app>
